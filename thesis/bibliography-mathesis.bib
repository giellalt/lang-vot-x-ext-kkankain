	langid = {english},
	keywords = {lexicon, Livonian, language contact, word formation, neologisms, borrowing, derivation, prefixes, suffixes, calques},
	file = {Full Text PDF:/home/kristian/Zotero/storage/45G2G7BI/Ernštreits - 2016 - Methods for expanding the Livonian lexicon.pdf:application/pdf;Snapshot:/home/kristian/Zotero/storage/BS6CF64T/jeful.2016.7.1.html:text/html}
