	keywords = {{HFST}, Livonian, Uralic languages, “Kone” language programme, open-source, language-independent infrastructure, morphological parser, spellcheckers, morphology-savvy web dictionary, intelligent computer-assisted language learning, Giellatekno},
	file = {Rueter - 2014 - The Livonian-Estonian-Latvian Dictionary as a thre.pdf:/home/kristian/Zotero/storage/T9ZKAZ3I/Rueter - 2014 - The Livonian-Estonian-Latvian Dictionary as a thre.pdf:application/pdf;Snapshot:/home/kristian/Zotero/storage/N7QTFIH4/jeful.2014.5.1.html:text/html}
}

