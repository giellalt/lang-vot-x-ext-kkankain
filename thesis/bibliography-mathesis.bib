@article{moseley_revitalizing_2016,
	title = {Revitalizing a community language: Livonian and other community languages},
	volume = {7},
	rights = {This work is licensed under a  Creative Commons Attribution-{NonCommercial}-{ShareAlike} 4.0 International License .},
