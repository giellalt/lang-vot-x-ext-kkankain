@inproceedings{papakonstantinou_dtd_1999,
	title = {{DTD} Inference for Views of {XML} Data},
	abstract = {We study the inference of Data Type Definitions ({DTDs}) for views of {XML} data, using an abstraction that focuses on document content structure. The views are defined by a query language that produces a list of documents selected from one or more input sources. The selection conditions involve vertical and horizontal navigation, thus querying explicitly the order present in input documents. We point several strong limitations in the descriptive ability of current {DTDs} and the need for extending them with (i) a subtyping mechanism and (ii) a more powerful specification mechanism than regular languages, such as context-free languages. With these extensions, we show that one can always infer tight {DTDs}, that precisely characterize a selection view on sources satisfying given {DTDs}. We also show important special cases where one can infer a tight {DTD} without requiring extension (ii). Finally we consider related problems such as verifying conformance of a view definition with a predefined {DTD}....},
	pages = {35--46},
